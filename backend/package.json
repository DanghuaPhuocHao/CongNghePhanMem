{
  "name": "ada_fastfood_backend",
  "version": "1.0.0",
  "description": "Backend for the Ada Fastfood application",
  "main": "src/app.js",
  "scripts": {
    "start": "node src/app.js",
    "build": "tsc",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^5.10.9",
    "multer": "^2.0.0",
    "swagger-ui-express": "^4.1.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.20",
    "@types/mongoose": "^5.10.2",
    "jest": "^26.6.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.3",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.2"
  },
  "jest": {
    "testEnvironment": "node",
    "roots": ["<rootDir>/src", "<rootDir>/__tests__"],
    "testMatch": [
      "**/__tests__/**/*.js",
      "**/?(*.)+(spec|test).js"
    ]
  },
  "author": "Your Name",
  "license": "MIT"
}
